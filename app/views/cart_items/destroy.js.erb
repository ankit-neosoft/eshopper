<% if @cart_items.count==0 %>
  $('#complete-cart-items').fadeOut()
  $('.collapse').show()
<% else %>
  $('#<%= @cart_item.id%>').hide()
<% end %>
$('#cart_counts').html('('+<%= @cart_items.count%>+')')
$('#cart_item_total_'+<%= @cart_item.id %>).html('&#8377;'+ <%= @cart_item.total %>.toFixed(1))
$('#vat').html('&#8377; <%= @vat%>')
$('#cart_sub_total').html('&#8377; <%= @cart_items_total%>')
$('#cart_total').html('&#8377; <%= @grand_total%>')